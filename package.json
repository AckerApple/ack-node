{
  "name": "ack-node",
  "version": "1.5.0",
  "description": "Common server-side functionality, wrapped in one package, the Acker way",
  "main": "index.js",
  "types": "js/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm-run-all test:mocha",
    "test:mocha": "mocha ./test/cases --recursive",
    "test:watch": "npm run test:mocha -- --watch",
    "build:js": "tsc ts/modules/** --outDir js --declaration true --lib es6",
    "watch:js": "npm run build:js -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AckerApple/ack-node.git"
  },
  "keywords": [
    "node",
    "enterprise"
  ],
  "author": "Acker Apple",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AckerApple/ack-node/issues"
  },
  "homepage": "https://github.com/AckerApple/ack-node#readme",
  "dependencies": {
    "ack-path": "^1.7.0",
    "ack-x": "^1.5.10",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-timeout": "^1.9.0",
    "cookies": "^0.7.3",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.6",
    "etag": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "multiparty": "^4.2.1",
    "node-schedule": "^1.3.2",
    "nodedump": "^0.0.8",
    "nodemailer": "^6.2.1",
    "pug": "^2.0.3",
    "request": "^2.88.0",
    "ua-parser-js": "^0.7.19"
  },
  "devDependencies": {
    "@types/node": "^10.7.1",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.0.1"
  }
}
